{
  "Serilog": {
    "Initialisation": {
      "MinimumLevel": {
        "Default": "Debug"
      },
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "outputTemplate": "INITIALISATION LOGGER: [{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
          }
        }
      ]
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "PostgreSqlOptions": {
    "Host": "localhost",
    "Username": "pixelparadise_user",
    "Password": "S3cret",
    "Port": 5432,
    "Database": "pixelparadise_db"
  },
  "StartupOptions": {
    "EnableSwagger": "true",
    "AllowCors": "true"
  },
  "AllowedHosts": "*"
}